<style type="text/css">
.form-horizontal .control-label {
	text-align: left;
}

.payment-hide {
	display: none;
}

.form-group {
	min-height: 110px
}

label.checkbox input[type="checkbox"] {
	border: 0 none;
	clip: rect(0px, 0px, 0px, 0px);
	height: 1px;
	margin: -1px;
	overflow: hidden;
	padding: 0;
	position: absolute;
	width: 1px;
}

label.checkbox  input[type="checkbox"]+span>span.tick, 
label.checkbox  input[type="checkbox"]:checked+span>span.tick
	{
	transition: opacity 0.125s linear 0s;
}

label.checkbox input[type="checkbox"] + span > span.tick {
    background-color: transparent;
    background-image: url('data:image/png;base64,');
    background-position: 3px 1px;
    background-repeat: no-repeat;
    background-size: 18px auto;
    display: block;
    height: 40px;
    left: -3px;
    opacity: 0;
    position: absolute;
    top: -3px;
    width: 40px;
    z-index: 100;
}

label.checkbox  input[type="checkbox"]:checked+span>span.tick {
	opacity: 1;
}

label.checkbox input[type="checkbox"]+span {
	background-color: #fff;
	border: 3px solid #dbdad4;
	cursor: pointer;
	display: inline-block;
	height: 40px;
	margin: -2px 10px 0 0;
	position: relative;
	vertical-align: middle;
	width: 40px;
}

table { 
    display: table;
    font-weight: normal;
    border: solid #f68b1f;
}
table.th { 
    font-weight: normal;
    font-family: Arial,sans-serif;
}
</style>

<div class="title margin-top-l">
	<h3>
		<spring:message code="register.title" />
	</h3>
</div>

<div class="margin-top-l" style="font-weight: normal;background-color: #f1f1f1;">
	<section style="background-color: #f1f1f1;">
		<form:form action="../public/register"
			modelAttribute="registerForm" class="form-horizontal ">
			<fieldset>
				<div class="col col-sm-12" style="padding: 5px;"><h4>Vui lòng nhập đầy đủ các thông tin sau:</h4></div>
				<div class="col col-sm-12" style="padding: 5px;">				
					<form:label path="fullName" class="col col-sm-4 control-label">Nhập Mã số Tư vấn tài chính</form:label>
					<div class="col col-sm-6">
						<form:input path="fullName" class="form-control" />																
						<form:errors path="fullName" cssClass="text-danger" />
					</div>
				</div>
				<div class="col col-sm-12" style="padding: 5px;">
					<form:label path="fullName" class="col col-sm-4 control-label">Chọn khi là nhân viên, khách mời</form:label>
					<div class="col col-sm-8">
						<input type="checkbox" id="option_1" name="option_1"/>
					</div>
				</div>			
				<div class="col col-sm-12 control-label" style="padding: 5px;">
				--------------------------------------------------------
				</div>
				<div class="col col-sm-12 control-label" style="font-weight: 700;padding: 15px;">
					<div style="padding: 5px;">Cuộc sống luôn thay đổi, <br></div>
					<div style="padding: 5px;">Và thay đổi có thể đến dưới nhiều hình thức trong cuộc sống của chúng ta. <br></div>
					<div style="padding: 5px;">Đó có thể là những sự kiện quan trọng, <br></div>
					<div style="padding: 5px;">&nbsp;&nbsp;&nbsp;những lựa chọn khó khăn, <br></div>
					<div style="padding: 5px;">&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;hoặc những cơ hội mà bạn chưa ngờ tới.<br>	</div>					
				</div>					
					<div class="col col-sm-12 control-label" style="border: solid #f68b1f">
						<span style="color: rgb(255, 255, 255);background-color: #f68b1f;">Nếu thay đổi sắp đến trong tương lai là cơ hội, vậy bạn sẽ chọn cơ hội nào sau đây:<br></span>
						<i>(Chỉ chọn 1 lựa chọn)</i>
					
					<div class="col col-sm-12">
						<label class="checkbox">
							<input type="radio" id="selection" name="selection" value="1 & 2"/>
							<span class="indicator"><span class="tick"></span></span>
							<span class="p3">
								A. Được đi du lịch, khám phá những vùng đất mới	
							</span>
						</label>									
					</div>
					<div class="col col-sm-12">
						<label class="checkbox">
							<input type="radio" id="selection" name="selection" value="4"/>
							<span class="indicator"><span class="tick"></span></span>
							<span class="p3">
							B. Trở thành người nổi tiếng, có sức ảnh hưởng
							</span>
						</label>										
					</div>
					<div class="col col-sm-12">
						<label class="checkbox">
							<input type="radio" id="selection" name="selection" value="5"/>
							<span class="indicator"><span class="tick"></span></span>
							<span class="p3">
							C. Thành công trong công việc và giàu có
							</span>
						</label>											
					</div>
					<div class="col col-sm-12">
						<label class="checkbox">
							<input type="radio" id="selection" name="selection" value="3"/>
							<span class="indicator"><span class="tick"></span></span>
							<span class="p3">
							D. Học tập và sinh sống ở nước ngoài
							</span>
						</label>											
					</div>
					</div>
				<div class="col col-sm-12 margin-top-l">
					<img src="../resources/img/Icon-07.png" alt="75" width="75" >
					<a href="#" onclick="validate()"
						class="btn btn-primary text-uppercase full-width" style="font-weight: 700;">Rút từ Hợp đồng bảo hiểm để nắm bắt NGAY cơ hội</a>
				</div>				

			</fieldset>
		</form:form>
	</section>
	<div class="margin-top-l"></div>
</div>
<script type="text/javascript">	
	function validate_fullname() {
		var fullname = $("#fullName").val();	
		var re = /^[a-zA-Z0-9\s]+$/;
		var checkboxCheck = $("input[name='option_1']").is(':checked');
		if (checkboxCheck) {
			$("#fullName_errors").text("");
			return true;
		} else if (fullname.length === 0 &&  ! checkboxCheck) {				
			$("#fullName_errors").text("Vui lòng điền mã số tư vấn hoặc chọn Khác (nhân viên, khách mời).");
			return false;
		} else if (! re.test(fullname) &&  ! checkboxCheck) {
			$("#fullName_errors").text("Vui lòng điền mã số tư vấn đúng định dạng với Chữ và số.");
			return false;
		} else if (fullname.length != 8){
			$("#fullName_errors").text("Vui lòng điền đúng mã số tư vấn với 8 chữ số.");
			return false;
		} else {
			$("#fullName_errors").text("");
			return true;
		}
		
	}
	
	function validateAll() {
		var fullname = $("#fullName").val();	
		var chkFlag = true;
		var checkboxCheck = $("input[name='option_1']").is(':checked');
		if (fullname.length === 0) {
			if(checkboxCheck) {
				$("#fullName_errors").text("");
				$("#fullName").val("KHACH"); 
				chkFlag = true;
			} else {
				$("#fullName_errors").text("Vui lòng điền mã số tư vấn hoặc chọn Khác (nhân viên, khách mời).");
				chkFlag = false;
			}
		} else {
			$('#option_1').attr('checked', false);	
		}
		
		if(chkFlag) {			
			return true;
		} else {
			return false;
		}
	}
	
	function validate() {
		var selectQuiz = $("input[name='selection']").is(':checked');		
		if (validateAll() && selectQuiz) {
			document.forms[0].submit();
		} else {
			alert("<spring:message code="register.cannotSubmit"/>");			
		}
	}

	$(function() {
		$("#fullName").parent().append($('<span id="fullName_errors" class="text-danger"></span>'));
		$("#fullName").blur(function() {			
			validate_fullname();
		});	
		$("#fullName").keyup(function() {			
			$('#option_1').attr('checked', false);	
		});	
		$("#option_1").click(function() {
			if($(this).prop("checked")) { 
				$("#fullName_errors").text("");
				$("#fullName").val(""); 
			}
		});
		
	});
</script>