<style type="text/css">
.form-horizontal .control-label {
	text-align: left;
}

.payment-hide {
	display: none;
}

.form-group {
	min-height: 90px
}

label.checkbox input[type="checkbox"] {
	border: 0 none;
	clip: rect(0px, 0px, 0px, 0px);
	height: 1px;
	margin: -1px;
	overflow: hidden;
	padding: 0;
	position: absolute;
	width: 1px;
}

label.checkbox  input[type="checkbox"]+span>span.tick, 
label.checkbox  input[type="checkbox"]:checked+span>span.tick
	{
	transition: opacity 0.125s linear 0s;
}

label.checkbox input[type="checkbox"] + span > span.tick {
    background-color: transparent;
    background-image: url('data:image/png;base64,');
    background-position: 3px 1px;
    background-repeat: no-repeat;
    background-size: 18px auto;
    display: block;
    height: 20px;
    left: -3px;
    opacity: 0;
    position: absolute;
    top: -3px;
    width: 20px;
    z-index: 100;
}

label.checkbox  input[type="checkbox"]:checked+span>span.tick {
	opacity: 1;
}

label.checkbox input[type="checkbox"]+span {
	background-color: #fff;
	border: 3px solid #dbdad4;
	cursor: pointer;
	display: inline-block;
	height: 20px;
	margin: -2px 10px 0 0;
	position: relative;
	vertical-align: middle;
	width: 20px;
}
div.fancy-select {
	border: 1px solid #ccc;
}
span.manda {
color: red;
}



.radio-item {
  display: inline-block;
  position: relative;
  padding: 0 6px;
  margin: 10px 0 0;
}

.radio-item input[type='radio'] {
  display: none;
}

.radio-item label {
  color: #575757;
  font-weight: normal;
}

.radio-item label:before {
  content: " ";
  display: inline-block;
  position: relative;
  top: 5px;
  margin: 0 5px 0 0;
  width: 20px;
  height: 20px;
  border-radius: 11px;
  border: 2px solid #f68b1f;
  background-color: transparent;
}

.radio-item input[type=radio]:checked + label:after {
  border-radius: 11px;
  width: 12px;
  height: 12px;
  position: absolute;
  top: 9px;
  left: 10px;
  content: " ";
  display: block;
  background: #f68b1f;
}

</style>


