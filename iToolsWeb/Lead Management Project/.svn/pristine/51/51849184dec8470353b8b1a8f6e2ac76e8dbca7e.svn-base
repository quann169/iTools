<style type="text/css">
.form-horizontal .control-label {
	text-align: left;
}

.payment-hide {
	display: none;
}

.form-group {
	min-height: 110px
}

label.checkbox input[type="checkbox"] {
	border: 0 none;
	clip: rect(0px, 0px, 0px, 0px);
	height: 1px;
	margin: -1px;
	overflow: hidden;
	padding: 0;
	position: absolute;
	width: 1px;
}

label.checkbox  input[type="checkbox"]+span>span.tick, 
label.checkbox  input[type="checkbox"]:checked+span>span.tick
	{
	transition: opacity 0.125s linear 0s;
}

label.checkbox input[type="checkbox"] + span > span.tick {
    background-color: transparent;
    background-image: url("data:image/png;base64,");
    background-position: 3px 1px;
    background-repeat: no-repeat;
    background-size: 18px auto;
    display: block;
    height: 20px;
    left: -3px;
    opacity: 0;
    position: absolute;
    top: -3px;
    width: 20px;
    z-index: 100;
}

label.checkbox  input[type="checkbox"]:checked+span>span.tick {
	opacity: 1;
}

label.checkbox input[type="checkbox"]+span {
	background-color: #fff;
	border: 3px solid #dbdad4;
	cursor: pointer;
	display: inline-block;
	height: 20px;
	margin: -2px 10px 0 0;
	position: relative;
	vertical-align: middle;
	width: 20px;
}
</style>

<div class="title margin-top-l">
	<h2>
		<spring:message code="register.titleSuccess" />
	</h2>
</div>

<div class="margin-top-l" style="background-color: #f1f1f1;">
	<section style="background-color: #f1f1f1;">
		<spring:message code="register.successMessage" /> <br> 
		<spring:message code="register.successMessageContact" />
	</section>
	<div class="margin-top-l"></div>
</div>